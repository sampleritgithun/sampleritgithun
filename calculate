first= input("enter first number:")
operator = input("enter operator (+,-,*,/,%)")
second= input("enter first number:")
first=int(first)
second=int(second)
if operator == "+":
    print(first+second)
elif operator == "-":
    print(first-second)
elif operator == "*":
    print(first*second)
elif operator == "/":
    print(first/second)
elif operator =="%":
    print(first%second)
